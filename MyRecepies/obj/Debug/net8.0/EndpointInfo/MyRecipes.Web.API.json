  "components": {
    "schemas": {
      "AccessTokenResponse": {
        "required": [
          "accessToken",
          "expiresIn",
          "refreshToken"
        ],
        "type": "object",
        "properties": {
          "tokenType": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "accessToken": {
            "type": "string",
            "nullable": true
          },
          "expiresIn": {
            "type": "integer",
            "format": "int64"
          },
          "refreshToken": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateFoodTypeModel": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateIngredientModel": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "ingredientCategoryId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "CreateInstructionModel": {
        "type": "object",
        "properties": {
          "recipeId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "step": {
            "type": "integer",
            "format": "int32"
          },
          "stepName": {
            "type": "string",
            "nullable": true
          },
          "stepInstruction": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateRecipeIngredientModel": {
        "type": "object",
        "properties": {
          "ingredientId": {
            "type": "string",
            "format": "uuid"
          },
          "quantity": {
            "type": "number",
            "format": "double"
          },
          "unit": {
            "$ref": "#/components/schemas/UnitOfMeasure"
          },
          "recipeId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateRecipeModel": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "recipyDifficulty": {
            "$ref": "#/components/schemas/Difficulty"
          },
          "timeToPrepareRecipe": {
            "type": "integer",
            "format": "int32"
          },
          "nbGuest": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Difficulty": {
        "enum": [
          1,
          2,
          3,
          4,
          5
        ],
        "type": "integer",
        "format": "int32"
      },
      "ForgotPasswordRequest": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "HttpValidationProblemDetails": {
        "type": "object",
